{
  "name": "ionic-react-starter-sidemenu",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@ionic/react": "^4.8.0-rc.0",
    "@ionic/react-router": "^4.8.0-rc.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "ionicons": "^4.6.2",
    "lint-staged": "^9.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci \"test:coverage:ci\" \"test:coverage:changed\"",
    "test:file": "react-scripts test --watchAll=false",
    "test:file:watch": "react-scripts test --watchAll",
    "test:coverage:changed": "react-scripts test --coverage",
    "test:coverage:ci": "react-scripts test --coverage --watchAll=false",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "eject": "react-scripts eject",
    "prettier": "prettier \"src/**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\" --write --check",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --max-warnings 0",
    "validate": "yarn lint && yarn test:coverage:ci"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add"
    ],
    "src/**/*.*": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Ionic Sidemenu React Starter",
  "devDependencies": {
    "@testing-library/react": "^9.1.3",
    "is-ci-cli": "^1.1.1",
    "jest-runner-eslint": "^0.7.4"
  }
}
